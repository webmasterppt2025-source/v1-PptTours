  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:general\" \"npm run dev:cabecera\"",
    "dev:general": "npx tailwindcss -i ./estilos/tailwind-general.css -o ./estilos/general.css --watch",
    "dev:cabecera": "npx tailwindcss -i ./estilos/tailwind-cabecera.css -o ./estilos/cabecera.css --watch",
    "build": "npm run build:general && npm run build:cabecera",
    "build:general": "npx tailwindcss -i ./estilos/tailwind-general.css -o ./estilos/general.css --minify",
    "build:cabecera": "npx tailwindcss -i ./estilos/tailwind-cabecera.css -o ./estilos/cabecera.css --minify"
  },
